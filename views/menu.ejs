<!DOCTYPE html>
<html lang="en">
<head>
  <title>Menu Page</title>
</head>

<%- include('partials/_header') -%>
<body>
	<!-- <%- include('partials/_nav')-%> -->
				<% for (food of menu) { %>
          <article class="menu">
            <section class="menu_preview">
              <ul style="list-style: none;">
                <div class="food_title">
                  <li class="food_name"><%= food.name %></li>
                  <button type="submit" class="btn btn-primary mb-3 ">Add to Cart</button>
                </div>


                <li><img class="food_image" src="<%= food.picture_url %>"></li>
                <li><%= food.description %></li>
                <li><%= food.price %></li>
              </ul>
            </section>
          </article>
				<% } %>
</body>




